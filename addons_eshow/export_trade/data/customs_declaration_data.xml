<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for customs declaration -->
        <record id="seq_customs_declaration" model="ir.sequence">
            <field name="name">Customs Declaration</field>
            <field name="code">sale.customs.declaration</field>
            <field name="prefix">CD%(y)s%(month)s%(day)s-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
