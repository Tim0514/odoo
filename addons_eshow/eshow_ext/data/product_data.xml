<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--
             Precisions
        -->
        <record forcecreate="True" id="decimal_length" model="decimal.precision">
            <field name="name">Length</field>
            <field name="digits">2</field>
        </record>

        <!--
            System Parameters
        -->
        <record forcecreate="True" id="maxshare_price_rate" model="ir.config_parameter">
            <field name="key">eshow_ext.maxshare_price_rate</field>
            <field name="value">1.4</field>
        </record>

        <record forcecreate="True" id="export_declare_price_rate" model="ir.config_parameter">
            <field name="key">eshow_ext.export_declare_price_rate</field>
            <field name="value">1.2</field>
        </record>

        <record forcecreate="True" id="destination_declare_price" model="ir.config_parameter">
            <field name="key">eshow_ext.destination_declare_price_rate</field>
            <field name="value">0.5</field>
        </record>

        <!--
        Property
        -->

    </data>
</odoo>
